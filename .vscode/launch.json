        // Run npm run set-dev from functions directory
        {
            "name": "Set firebase development project",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}/functions",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script",
                "set-dev"
            ],
        },
        // Run npm run set-prod from functions directory
        {
            "name": "Set firebase production project",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}/functions",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script",
                "set-prod"
            ],
        },
        // Run npm run deploy-ff from functions directory
        {
            "name": "Deploy firebase functions and firestore rules",
            "type": "node",
            "request": "launch",
