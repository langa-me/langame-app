            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script",
                "deploy-ff"
            ],
        },
        // Run npm run compile from functions directory
        {
            "name": "Enable automatic compilation for running tests locally",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}/functions",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
