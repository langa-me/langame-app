{
  "name": "functions",
  "description": "Firebase & co",
  "scripts": {
    "lint": "eslint --ext .ts .",
    "build": "tsc",
    "serve": "npm run build -- --watch | firebase emulators:start",
    "serve-nowatch": "firebase emulators:start",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy",
    "logs": "firebase functions:log",
    "set-dev": "firebase use langame-dev",
    "set-prod": "firebase use langame-86ac4",
    "test": "nyc --reporter=lcov mocha -r ts-node/register --reporter spec test/stripe.test.ts --timeout 30000",
    "coverage": "codecov"
  },
  "engines": {
    "node": "12"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/logging": "^9.2.3",
    "@octokit/rest": "^18.5.2",
    "agora-access-token": "^2.0.1",
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.11.0",
    "mailchimp-api-v3": "^1.15.0",
    "node-fetch": "^2.6.1",
    "stripe": "^8.148.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/cors": "^2.8.10",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.0.11",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "chai": "^4.3.4",
    "codecov": "^3.8.2",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-functions-test": "^0.2.3",
    "firebase-tools": "^9.11.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "portfinder": "^1.0.28",
    "sinon": "^10.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "private": true
}
