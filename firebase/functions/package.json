    "@types/jest": "^26.0.22",
    "@types/node": "14.0.11",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.0",
    "ts-jest": "^24.0.2",
    "typescript": "^4.2.3"
